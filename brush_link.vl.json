{ 
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {"url": "student-mat.csv"},
  "vconcat": [{
    "selection": {
      "brush": {"type": "interval"}
    },
    "mark": "point",
    "encoding": {
      "x": {"field": "studytime","type": "quantitative"},
      "y": {"field": "G3","type": "quantitative"}
    }
  }, {
    "transform": [
      {"filter": {"selection": "brush"}}
    ],
    "mark": "point",
    "encoding": {
      "x": {
        "field": "Dalc", "type": "quantitative",
        "scale": {"domain": [1,5]}
      },
      "y": {
        "field": "health","type": "quantitative",
        "scale": {"domain": [1, 5]}
      }
    }
  }]
}

